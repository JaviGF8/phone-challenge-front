{
  "env": {
      "production": {
          "presets": [
              "@babel/preset-env",
              "@babel/preset-react",
              "minifi"
          ],
          "plugins": [
              "transform-class-properties"
          ]
      },
      "development": {
          "presets": [
              "@babel/preset-env",
              "@babel/preset-react"
          ],
          "plugins": [
              "transform-class-properties"
          ]
      },
      "test": {
        "presets": [
            "@babel/env",
            "@babel/react"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties",
            "@babel/plugin-proposal-object-rest-spread",
            "transform-class-properties"
        ]
      }
  }
}